  "repository": {
    "type": "git",
    "url": "git+https://github.com/kermankohli/natural-language-date-parser.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "luxon": "^3.5.0",
    "uuid": "^11.0.5",
    "winston": "^3.17.0"
